a = 0.08
b = 250
c = 100
d = 0.15

#fitted
a1 = 0.0616216577
b1 = 256.093245
c1 = 106.807773
d1 = 0.149160992

g(x) = a*exp(-(((x-b)**2)/((2*c)**2))) + d
f(x) = a1*exp(-(((x-b1)**2)/((2*c1)**2))) + d1

set xrange [-50:750]
set yrange [0:0.23]

p g(x) t "estimated", f(x) t "fitted"
replot "results/RyR/P2_plot.dat" u 1:2
#replot "results/RyR/RSDr000_avg/ca_avg.dat" u 1:3 w l
